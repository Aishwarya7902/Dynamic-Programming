#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int countset(int *arr,int sum,int n){
    int  t[n+1][sum+1];
    for(int i=0;i<n+1;i++){
        for(int j=0;j<sum+1;j++){
            if(i==0){
                t[i][j]=0;
            }
            if(j==0)t[i][j]=1;
            
        }
    }
for(int  i=1;i<=n;i++){
    for(int j=1;j<=sum;j++){
        if(arr[i-1]<=sum){
            t[i][j]=t[i-1][j]+t[i-1][j-arr[i-1]];
        }
        else{
            t[i][j]=t[i-1][j];
        }
    }
}
return t[n][sum];
}
int main()
{
    int arr[] = { 3, 3, 3, 3 };
    int n = sizeof(arr) / sizeof(int);
    int x = 6;
 
    cout <<countset(arr,x,n);
 
    return 0;
}
